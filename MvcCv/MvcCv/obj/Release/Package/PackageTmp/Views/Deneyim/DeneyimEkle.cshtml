
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Yeni Deneyim Ekleme Sayfası</h2>
<br />
<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Yazınız" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="AltBaşlığı Yazınız" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarih Yazınız" />
    <br />
    <textarea placeholder="Açıklama Girinniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

